{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=development tsx server/index.ts",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:e2e": "playwright test",
    "test": "npm run test:unit && npm run test:e2e",
    "test:performance": "NODE_OPTIONS='--expose-gc --max-old-space-size=4096' vitest run tests/performance --reporter=verbose",
    "test:performance:watch": "NODE_OPTIONS='--expose-gc --max-old-space-size=4096' vitest tests/performance",
    "test:perf:batch": "NODE_OPTIONS='--expose-gc --max-old-space-size=4096' vitest run tests/performance/large-batch.perf.test.ts",
    "test:perf:users": "NODE_OPTIONS='--expose-gc --max-old-space-size=4096' vitest run tests/performance/concurrent-users.perf.test.ts",
    "test:perf:websocket": "NODE_OPTIONS='--expose-gc --max-old-space-size=4096' vitest run tests/performance/websocket-throughput.perf.test.ts",
    "test:perf:memory": "NODE_OPTIONS='--expose-gc --max-old-space-size=4096' vitest run tests/performance/memory-leak.perf.test.ts",
    "test:perf:database": "NODE_OPTIONS='--expose-gc --max-old-space-size=4096' vitest run tests/performance/database-query.perf.test.ts",
    "test:perf:upload": "NODE_OPTIONS='--expose-gc --max-old-space-size=4096' vitest run tests/performance/file-upload.perf.test.ts",
    "run:openai": "node scripts/run-openai.js",
    "start": "NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "lint": "tsc --noEmit",
    "test:native:mac": "swift test --package-path apps/mac-menubar --enable-test-discovery",
    "test:native:mobile": "swift test --package-path apps/mobile-companion --enable-test-discovery",
    "test:native": "npm run test:native:mac && npm run test:native:mobile",
    "test:all": "npm run test:unit && npm run test:e2e && npm run test:native",
    "generate:swift-schemas": "npx tsx scripts/generate-swift-schemas.ts"
  },
